@model BooksListViewModel
<h1>Все книги</h1>
<h3 class="text-center">@Model.currCategory</h3>
<div class="d-flex justify-content-center">
    <div class="row mt-3 mb-2">
        @{
            foreach (var book in Model.allBooks)
            {
                @Html.Partial("AllBooks", book)
            }
        }
    </div>
</div>
